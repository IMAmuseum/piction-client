{
    "name": "imamuseum/piction-client",
    "require": {
        "php" : ">=5.4.0",
        "vlucas/phpdotenv": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Imamuseum\\PictionClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imamuseum\\PictionClient\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
